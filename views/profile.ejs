   <!DOCTYPE html>
   <title>Medagascar</title>
   
   <head>
  
      <script src="https://kit.fontawesome.com/b99e675b6e.js"></script>
      <link rel="stylesheet" href="/css/profile-style.css">
   
   </head>
   
   <body>
      <div class="wrapper">
         <div class="sidebar">
            <div class="logo">
               <h2>Medagascar</h2>
            </div>
            <br><br><br>
            <ul>
               <li><a href="/profile"><i class="fas fa-user"></i>Profile</a></li>
               <li><a href="/upload"><i class="fas fa-upload"></i>Upload</a></li>
               <li><a href="/prescription"><i class="fas fa-prescription"></i>Prescription</a></li>
               <li><a href="/logout"><i class="fas fa-sign-out-alt"></i>Sign Out</a></li>
               <li><a href="/emergency"><i class="fas fa fa-medkit" aria-hidden="true"></i>Emergency</a></li>
             
               
            </ul>
         </div>
      </div>

   
      <div class="page-content page-container" id="page-content">
         <div class="padding">
            <div class="row container d-flex justify-content-center">
               <div class="col-xl-6 col-md-12">
                  <div class="card user-card-full">
                     <div class="row m-l-0 m-r-0">
                        <div class="col-sm-4 bg-c-lite-green user-profile">
                           <div class="card-block text-center text-white">
                              <div class="m-b-35">
                                 <img src="https://img.icons8.com/bubbles/100/000000/user.png" class="img-radius"
                                    alt="User-Profile-Image">
                              </div>
                              <h1 class="f-w-500"><%=user%></h1>
                              <i class=" mdi mdi-square-edit-outline feather icon-edit m-t-10 f-16"></i>
                           </div>
                        </div>
                        <div class="col-sm-8">
                           <div class="card-block">
                              <h2 class="m-b-20 p-b-5 b-b-default f-w-600">Information:</h2>
                              <div class="row">
                                 <div class="col-sm-6">
                                    <h3 class="m-b-10 f-w-600">Age</h3>
                                    <h4 class="text-muted f-w-400"><%=age%></h4><br>
                                 </div>
                                 <div class="col-sm-6">
                                    <h3    class="m-b-10 f-w-600">Gender</h3>
                                    <h4 class="text-muted f-w-400"><%=gender%></h4><br>
                                 </div>
                              </div>
   
                              <div class="row">
                                 <div class="col-sm-6">
                                    <h3 class="m-b-10 f-w-600">Allergies:</h3>
                                    <% allergies.forEach(function(allergy){ %>
                                       <h4 class="text-muted f-w-400"><%=allergy.allergy%></h4><br>
                                    <% }); %>
                                 </div>
                                 <div class="col-sm-6">
                                    <h3 class="m-b-10 f-w-600">Medicines on:</h3>
                                    <% medicines.forEach(function(medicine){ %>
                                       <h4 class="text-muted f-w-400">
                                          <%=medicine.medicine%>
                                       </h4><br>
                                       <% }); %>
                                 </div>
                                 <div class="col-sm-6">
                                    <h3 class="m-b-10 f-w-600">Family History:</h3>
                                    <% family.forEach(function(fam){ %>
                                       <h4 class="text-muted f-w-400">
                                          <%=fam.disease%>
                                       </h4><br>
                                       <% }); %>
                                 </div>

                                 <!-- <div class="col-sm-6">
                                    <h3 class="m-b-10 f-w-600">Possible Diseases</h3>
                                    <% family.forEach(function(fam){ %>
                                       <h4 class="text-muted f-w-400">
                                          <%=fam.disease%>
                                       </h4><br>
                                       <% }); %>
                                 </div> -->
                                 
                              </div>
                           </div>
                        </div>
                     </div>
                  </div>
               </div>
            </div>
         </div>
      </div>
   
   </body>
   
   </html>